{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "../build/dist/server",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true
  },
  "paths": {
    "*": [
        "node_modules/*",
        "src/types/*"
    ]
},
  "include": ["./**/*"],
}
